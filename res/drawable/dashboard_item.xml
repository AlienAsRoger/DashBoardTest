<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:drawable="@drawable/dashboard_item_pressed" android:state_pressed="true" />
    <item >
        <shape
            android:dither="true"
            android:shape="rectangle" >
            <solid android:color="@color/dashboard_item_default" />

            <stroke
                android:width="1px"
                android:color="@color/dashboard_item_stroke" />
        </shape>        
    </item>
    
</selector>