                android:drawableTop="@drawable/dashboard_item"
                android:text="Play Live" />
        </FrameLayout>

        <FrameLayout 
            android:id="@id/videoLessonsFrame"
            style="@style/DashboardItem" >

            <Button
                android:id="@id/videoLessons"
                style="@style/DashboardButton"
                android:drawableTop="@drawable/dashboard_item"
                android:text="Play Live" />
        </FrameLayout>

        <FrameLayout 
            android:id="@id/logOutFrame"
            style="@style/DashboardItem" >

            <Button
                android:id="@id/logOut"
                style="@style/DashboardButton"
